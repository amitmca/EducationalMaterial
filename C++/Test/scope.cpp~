//Program for reinterpret_cast operator

#include<iostream.h>
#include "conio.h"

int m=10;		//Global 

int main()
	{
	int m=20;
	clrscr();
		{
		int k=m;
		int m=30;
		cout<<"k="<<k<<"\n";
		cout<<"m="<<m<<"\n";
		cout<<"::m="<<::m<<"\n";
		}
	cout<<" m="<<m<<"\n";
	cout<<"::m="<<::m<<"\n";
	getch();
	return 0;
	}
